      </cac:PartyIdentification>
      <cac:PostalAddress>
        <cbc:StreetName>sample_StreetName</cbc:StreetName>
        <cbc:BuildingNumber>sample_BuildingNumber</cbc:BuildingNumber>
        <cbc:PlotIdentification>sample_PlotIdentification</cbc:PlotIdentification>
        <cbc:CitySubdivisionName>sample_CitySubdivisionName</cbc:CitySubdivisionName>
        <cbc:CityName>sample_CityName</cbc:CityName>
